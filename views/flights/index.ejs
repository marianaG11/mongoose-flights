<%- include('../partials/header') %>
<h1>Flight List</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
        <% flights.forEach(f => { %>
            <!-- Write the line of EJS to iterate over flights using forEach -->
        <tr>
            <td><%= f.airline %></td>
            <td><%= f.airport %></td>
            <td><%= f.flightNumber %></td>
            <td><%= f.departs %></td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</body>
</html>